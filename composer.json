{
    "name": "dibk/sync2pureservice",
    "description": "Støttefunksjoner for Pureservice, bygd på Laravel Zero-rammeverket.",
    "keywords": ["pureservice", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/DirektoratetForByggkvalitet/sync2pureservice",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/DirektoratetForByggkvalitet/sync2pureservice/issues",
        "source": "https://github.com/DirektoratetForByggkvalitet/sync2pureservice"
    },
    "authors": [
        {
            "name": "Eirik Wulff",
            "email": "ew@dibk.no"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "cardinalby/content-disposition": "^1.1",
        "illuminate/bus": "^11.21",
        "illuminate/database": "^11.5",
        "illuminate/http": "^11.5",
        "illuminate/mail": "^11.21",
        "illuminate/notifications": "^11.21",
        "illuminate/queue": "^11.21",
        "illuminate/redis": "^11.5",
        "illuminate/routing": "^11.21",
        "illuminate/translation": "^11.21",
        "illuminate/view": "^11.21",
        "innoge/laravel-msgraph-mail": "^1.3",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^11.0.0",
        "microsoft/microsoft-graph": "^2.17",
        "phpoffice/phpspreadsheet": "^2.2",
        "predis/predis": "^2.2",
        "symfony/uid": "^7.1",
        "zanysoft/laravel-zip": "^3.0"
    },
    "repositories": [

    ],    
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["sync2pureservice"]
}
